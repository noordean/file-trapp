<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @upload.name %>
</p>

<p>
  <strong>Files:</strong>
  <%= @upload.files.each do |file|%>
    <%= link_to file.filename, file, target: :blank %>
  <% end %>
</p>

<%= link_to 'Edit', edit_upload_path(@upload) %> |
<%= link_to 'Back', uploads_path %>
